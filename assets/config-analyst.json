{
  "heading": "2024 Analyst Form",
  "logo": "gemslogo.png",
  "pages": [
    {
      "name": "Setup",
      "widgets": [
        {
          "name": "Analyst Name",
          "type": "radio",
          "options": [
            "Carrie",
            "Sean",
            "Cooper",
            "Sam",
            "Other"
          ],
          "default": 4
        }
      ]
    },
    {
      "name": "Match",
      "widgets": [
        {
          "name": "Amp Rating",
          "type": "number",
          "validation": {
            "comparison": "inRange",
            "value": [1, 3]
          }
        },
        {
          "name": "Driver Rating",
          "type": "number",
          "validation": {
            "comparison": "inRange",
            "value": [1, 3]
          }
        },
        {
          "name": "Shooting Ranges",
          "type": "multicheckbox",
          "options": [
            "Subwoofer",
            "Starting line",
            "Podium",
            "Mid-stage",
            "Back-stage"
          ]
        },
        {
          "name": "Spotlighting?",
          "type": "checkbox"
        }
      ]
    },
    {
      "name": "Analysis",
      "widgets": [
        {
          "name": "Notes",
          "type": "textarea"
        }
      ]
    }
  ]
}
